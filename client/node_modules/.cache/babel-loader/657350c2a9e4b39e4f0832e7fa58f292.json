itle = '';\n    },\n    editTask: function editTask() {\n      console.log(\"Task Editted\");\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AA2JA;AACAA,cADA;AAEAC,MAFA,kBAEA;AACA;AACAC,0BADA;AAEAC,6BAFA;AAGAC,sBAHA;AAIAC,yBAJA;AAKAC,cACA;AACAC,aADA;AAEAC,6BAFA;AAGAC,8EAHA;AAIAC;AAJA,OADA,EAOA;AACAH,aADA;AAEAC,sBAFA;AAGAC,8DAHA;AAIAC;AAJA,OAPA,EAaA;AACAH,aADA;AAEAC,oBAFA;AAGAC,oDAHA;AAIAC;AAJA,OAbA;AALA;AA0BA,GA7BA;AA8BAC;AACAC,YADA,oBACAL,EADA,EACA;AACA;AAAA;AAAA;AACAM;AACA,KAJA;AAKAC,cALA,sBAKAP,EALA,EAKA;AACA;AAAA;AAAA;AACA,KAPA;AAQAQ,WARA,qBAQA;AACA;AACAR,sBADA;AAEAC,gCAFA;AAGAC,sCAHA;AAIAC;AAJA;AAMA;AACA;AACA;AACA,KAlBA;AAmBAM,YAnBA,sBAmBA;AACAC;AACA;AArBA;AA9BA","names":["name","data","edittedTaskTitle","edittedTaskSubtitle","newTaskTitle","newTaskSubtitle","tasks","id","title","subtitle","done","methods","doneTask","task","deleteTask","addTask","editTask","console"],"sourceRoot":"src/views","sources":["Todo.vue"],"sourcesContent":["<template>\n  <div class=\"todo\" >\n    <v-list\n      flat\n      three-line\n    >\n      <v-subheader>General</v-subheader>\n        <v-list-item\n        @click=\"doneTask(task.id)\"\n        :class=\"{'blue lighten-5' : task.done}\"\n        v-for=\"task in tasks\" \n        :key=\"task.id\"\n        >\n    \n          <template v-slot:default>\n            <v-list-item-action>\n              <v-checkbox \n              :input-value=\"task.done\">\n              </v-checkbox>\n            </v-list-item-action>\n\n            <v-list-item-content>\n              <v-list-item-title :class=\"{'text-decoration-line-through' : task.done}\">{{task.title}}</v-list-item-title>\n              <v-list-item-subtitle>{{task.subtitle}}</v-list-item-subtitle>\n            </v-list-item-content>\n\n          <v-menu\n            bottom\n            left\n          >\n            <template v-slot:activator=\"{ on, attrs }\">\n              <v-btn\n                icon\n                v-bind=\"attrs\"\n                v-on=\"on\"\n              >\n                <v-icon>mdi-dots-vertical</v-icon>\n              </v-btn>\n            </template>\n            <v-list>\n              <v-list-item\n              >\n              <v-list-item-action>\n                <v-dialog\n                  v-model=\"dialog\"\n                  max-width=\"500px\"\n                  max-height=\"700px\"\n                >\n              <template v-slot:activator=\"{ on, attrs }\"> \n              <v-btn \n                icon\n                v-bind=\"attrs\"\n                v-on=\"on\"\n              >\n                  <v-icon color=\"grey lighten-1\">mdi-magic-staff</v-icon>\n              </v-btn>\n               </template>\n               <v-card >\n                  <v-subheader class=\"font-weight-medium\" >Edit Task</v-subheader>\n                  <v-card-text>\n                      <v-text-field\n                        v-model=\"TaskTitle\"\n                        label=\"Title\"\n                        clearable\n                      >\n                      </v-text-field>\n                      <v-text-field\n                      v-model=\"TaskSubtitle\"\n                      label=\"Subtitle\"\n                      clearable\n                      >\n                      </v-text-field>\n                      <v-card-actions>\n                      <v-spacer></v-spacer>\n                      <v-btn\n                      text\n                      color=\"primary\"\n                      @click=\"editTask\"\n                      >\n                      Submit\n                      </v-btn>\n                      </v-card-actions>\n                      </v-card-text>\n                      </v-card>\n                      </v-dialog>\n              </v-list-item-action>\n              <v-list-item-action>\n              <v-btn \n                @click.stop=\"deleteTask(task.id)\"\n               icon>\n                  <v-icon color=\"grey lighten-1\">mdi-nuke</v-icon>\n              </v-btn>\n            </v-list-item-action>\n              </v-list-item>\n            </v-list>\n          </v-menu>\n          </template>\n        </v-list-item>\n    </v-list>\n    <v-container fluid>       \n      <v-dialog\n      v-model=\"dialog\"\n      max-width=\"500px\"\n      max-height=\"700px\"\n      >\n      <template v-slot:activator=\"{ on, attrs }\"> \n            <v-btn \n              slot = \"activator\" \n              fab\n              color=\"accent\"\n              elevation=\"9\"\n              absolute\n              :style=\"{bottom: '10%', left: '85%', transform:'translateX(-50%)'}\"\n              bottom\n              v-bind=\"attrs\"\n              v-on=\"on\"\n            >\n            <v-icon>mdi-plus</v-icon>\n            </v-btn>\n            </template>\n        <v-card >\n        <v-subheader class=\"font-weight-medium\" >Add Task</v-subheader>\n\n          <v-card-text>\n            <v-text-field\n            v-model=\"newTaskTitle\"\n            label=\"Title\"\n            clearable\n          >\n          </v-text-field>\n          <v-text-field\n            v-model=\"newTaskSubtitle\"\n            label=\"Subtitle\"\n            clearable\n          >\n          </v-text-field>\n             <v-card-actions>\n              <v-spacer></v-spacer>\n              <v-btn\n                text\n                color=\"primary\"\n                @click=\"addTask\"\n                @keyup.enter=\"addTask\"\n              >\n                Submit\n              </v-btn>\n            </v-card-actions>\n          </v-card-text>\n          </v-card>\n        </v-dialog>\n     </v-container>\n  </div>\n</template>\n\n<script>\n  export default {\n    name: 'Home',\n    data(){\n      return{\n        edittedTaskTitle: '',\n        edittedTaskSubtitle: '',\n        newTaskTitle: '',\n        newTaskSubtitle: '',\n        tasks:[\n          {\n            id: 1,\n            title: 'Notifcations',\n            subtitle: 'Notify me about updates to apps or games that I downloaded',\n            done: false\n          },\n          {\n            id: 2,\n            title: 'Super',\n            subtitle: 'updates to apps or games that I downloaded',\n            done: false\n          },\n          {\n            id: 3,\n            title: 'Man',\n            subtitle: ' apps or games that I downloaded',\n            done: false\n          }\n        ]\n      }\n    },\n    methods: {\n      doneTask(id){\n        let task = this.tasks.filter(task => task.id == id)[0]\n        task.done = !task.done\n      },\n      deleteTask(id){\n        this.tasks = this.tasks.filter(task => task.id !== id)\n      },\n      addTask(){\n        let newTask = {\n          id: Date.now(),\n          title: this.newTaskTitle,\n          subtitle: this.newTaskSubtitle,\n          done: false\n        }\n        this.tasks.push(newTask)\n        this.newTaskTitle = ''\n        this.newTaskSubtitle = ''\n      },\n      editTask(){\n          console.log(\"Task Editted\")\n        }\n         }\n        }\n\n  \n</script>\n"]},"metadata":{},"sourceType":"module"}