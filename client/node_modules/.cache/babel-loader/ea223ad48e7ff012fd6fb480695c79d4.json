{"ast":null,"code":"import \"core-js/modules/es.symbol.js\";\nimport \"core-js/modules/es.symbol.description.js\";\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\n//\nimport TutorialDataService from \"../services/TutorialDataService\";\nexport default {\n  name: \"add-tutorial\",\n  data: function data() {\n    return {\n      tutorial: {\n        id: null,\n        title: \"\",\n        description: \"\",\n        published: false\n      },\n      submitted: false\n    };\n  },\n  methods: {\n    saveTutorial: function saveTutorial() {\n      var _this = this;\n\n      var data = {\n        title: this.tutorial.title,\n        description: this.tutorial.description\n      };\n      TutorialDataService.create(data).then(function (response) {\n        _this.tutorial.id = response.data.id;\n        console.log(response.data);\n        _this.submitted = true;\n      }).catch(function (e) {\n        console.log(e);\n      });\n    },\n    newTutorial: function newTutorial() {\n      this.submitted = false;\n      this.tutorial = {};\n    }\n  }\n};","map":{"version":3,"mappings":";;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;AAoCA;AACA;AACAA,sBADA;AAEAC,MAFA,kBAEA;AACA;AACAC;AACAC,gBADA;AAEAC,iBAFA;AAGAC,uBAHA;AAIAC;AAJA,OADA;AAOAC;AAPA;AASA,GAZA;AAaAC;AACAC,gBADA,0BACA;AAAA;;AACA;AACAL,kCADA;AAEAC;AAFA;AAIAK,uCACAC,IADA,CACA;AACA;AACAC;AACA;AACA,OALA,EAMAC,KANA,CAMA;AACAD;AACA,OARA;AASA,KAfA;AAgBAE,eAhBA,yBAgBA;AACA;AACA;AACA;AAnBA;AAbA","names":["name","data","tutorial","id","title","description","published","submitted","methods","saveTutorial","TutorialDataService","then","console","catch","newTutorial"],"sourceRoot":"src/components","sources":["AddTutorial.vue"],"sourcesContent":["<template>\r\n  <div class=\"submit-form mt-3 mx-auto\">\r\n    <p class=\"headline\">Add Tutorial</p>\r\n    <div v-if=\"!submitted\">\r\n      <v-form ref=\"form\" lazy-validation>\r\n        <v-text-field\r\n          v-model=\"tutorial.title\"\r\n          :rules=\"[(v) => !!v || 'Title is required']\"\r\n          label=\"Title\"\r\n          required\r\n        ></v-text-field>\r\n        <v-text-field\r\n          v-model=\"tutorial.description\"\r\n          :rules=\"[(v) => !!v || 'Description is required']\"\r\n          label=\"Description\"\r\n          required\r\n        ></v-text-field>\r\n      </v-form>\r\n      <v-btn color=\"primary\" class=\"mt-3\" @click=\"saveTutorial\">Submit</v-btn>\r\n    </div>\r\n    <div v-else>\r\n      <v-card class=\"mx-auto\">\r\n        <v-card-title>\r\n          Submitted successfully!\r\n        </v-card-title>\r\n        <v-card-subtitle>\r\n          Click the button to add new Tutorial.\r\n        </v-card-subtitle>\r\n        <v-card-actions>\r\n          <v-btn color=\"success\" @click=\"newTutorial\">Add</v-btn>\r\n        </v-card-actions>\r\n      </v-card>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport TutorialDataService from \"../services/TutorialDataService\";\r\nexport default {\r\n  name: \"add-tutorial\",\r\n  data() {\r\n    return {\r\n      tutorial: {\r\n        id: null,\r\n        title: \"\",\r\n        description: \"\",\r\n        published: false,\r\n      },\r\n      submitted: false,\r\n    };\r\n  },\r\n  methods: {\r\n    saveTutorial() {\r\n      var data = {\r\n        title: this.tutorial.title,\r\n        description: this.tutorial.description,\r\n      };\r\n      TutorialDataService.create(data)\r\n        .then((response) => {\r\n          this.tutorial.id = response.data.id;\r\n          console.log(response.data);\r\n          this.submitted = true;\r\n        })\r\n        .catch((e) => {\r\n          console.log(e);\r\n        });\r\n    },\r\n    newTutorial() {\r\n      this.submitted = false;\r\n      this.tutorial = {};\r\n    },\r\n  },\r\n};\r\n</script>\r\n<style>\r\n.submit-form {\r\n  max-width: 300px;\r\n}\r\n</style>\r\n"]},"metadata":{},"sourceType":"module"}